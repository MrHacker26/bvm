{
  "name": "bvm",
  "version": "1.0.0",
  "description": "Bun Version Manager",
  "bin": {
    "bvm": "./dist/src/bvm.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/bvm.js",
    "dev": "node --no-warnings --loader ts-node/esm src/index.ts",
    "format": "prettier --write --ignore-path .gitignore --cache .",
    "lint": "eslint --ext .ts src --cache",
    "lint:fix": "eslint --ext .ts src --cache --fix"
  },
  "keywords": [
    "bun",
    "version",
    "manager"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^13.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.27.0",
    "globals": "^16.1.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.32.1"
  }
}
